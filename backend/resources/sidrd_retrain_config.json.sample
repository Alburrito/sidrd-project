{
    "tokenizer": {
        "default_mode": "stem",
        "extra_characters": [".", ",", ":", ";", "!", "?", "(", ")", "[", "]", "{", "}", "&", "*", "@", "#", "$", "%", "^", "+", "=", "|", "\\", "/", "~", "`", "<", ">", "\"", "'", "-"],
        "url_forbidden_chars": [".", ",", ":", ";", "!", "?", "(", ")", "[", "]", "{", "}", "&", "*", "@", "#", "$", "%", "^", "+", "=", "|", "\\", "/", "~", "`", "<", ">", "\"", "'", "-"],
        "custom_words": ["info", "https", "http", "org", "com", "net", "edu"],
        "lemmatizer_pass_tokens": ["js"],
        "stemmer_pass_tokens": ["js"]
    },
    "vectorizer": {},
    "clusterizer": {
        "n_clusters": 2,
        "limit": 5,
        "mode": "RECURSIVITY_LEVEL",
        "min_reports_per_cluster": 3
    },
    "classifier": {
        "param_grid": {
            "n_estimators": [10, 50, 100, 200, 500],
            "max_depth": [null, 2, 5, 10],
            "criterion": ["gini", "entropy"],
            "random_state": [42]
        },
        "per_duplicate_pairs": 0.20
    }
}